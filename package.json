{
  "name": "car-maintenance-management",
  "version": "0.1.0",
  "private": true,
  "dependencies": {},
  "scripts": {
    "install": "node scripts.js install",
    "i": "yarn install",
    "build": "node scripts.js build",
    "build:server": "node scripts.js build:server",
    "build:web": "node scripts.js build:web",
    "dev:web": "node scripts.js dev:web",
    "dev:w": "yarn dev:web",
    "dev:server": "nodemon -e go --watch ./server --inspect ./scripts.js dev:server",
    "dev:s": "yarn dev:server"
  },
  "devDependencies": {
    "nodemon": "^2.0.6",
    "prettier": "2.2.0"
  },
  "nodemonConfig": {
    "ignore": [
      "*.log",
      "build/*",
      "node_modules/*"
    ],
    "delay": 2500
  }
}
